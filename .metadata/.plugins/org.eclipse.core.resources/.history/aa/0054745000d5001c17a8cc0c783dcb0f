	public int getAge()
	{
		return age ;
	}
	public String getName()
	{
		return name  ;
	}
	public String getidName()
	{
		return idName  ;
	}
	
	public void setAge(int newAge) 
	{
		age = newAge ;
	}
	public void setName(String newName) 
	{
		age = newAge ;
	}
	

}
